{ "name": "@g-ui/state-manager",
  "version": "1.0.0",
  "description": "The portfolio module is responsible for translating current and history data about trades, transactions, and asset prices to produce data around value/gain/loss over time series.",
  "main": "src/index.ts",
  "license": "MIT",
  "jest": {
    "collectCoverage": true,
    "mapCoverage": true,
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/spec/.*.spec.ts|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testResultsProcessor": "jest-junit",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "dependencies": {
    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "camelcase-keys": "^4.2.0",
    "is-promise": "^2.1.0",
    "lodash": "^4.17.4",
    "lru_map": "^0.3.3",
    "moment": "^2.22.0",
    "qs": "^6.5.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.8.0",
    "reselect": "^3.0.1",
    "snakecase-keys": "^1.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "lint": "tslint --fix --project .",
    "test": "jest --coverage",
    "watch": "jest --watch",
    "generate-docs": "typedoc --out docs src --theme markdown",
    "transcribe": "tsc",
    "build": "yarn generate-docs && yarn transcribe"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "faker": "^4.1.0",
    "jest": "^22.1.4",
    "jest-junit": "^3.6.0",
    "ts-jest": "^22.0.2",
    "ts-loader": "^3.4.0",
    "tsc": "^1.20150623.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.10.0",
    "typedoc-plugin-markdown": "^1.0.14",
    "webpack": "^3.10.0"
  }
}
